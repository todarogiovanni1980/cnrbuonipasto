<?xml version="1.0" encoding="utf-8"?>
<form>
  <fieldset>
    <field name="id" type="text" readonly="true" class="readonly" default="0" description="JGLOBAL_FIELD_ID_DESC"/>
    <field name="ordering"/>
    <field name="state" default="1" type="list" label="JSTATUS" description="JFIELD_PUBLISHED_DESC" class="inputbox" size="1">
      <option value="1">JPUBLISHED</option>
      <option value="0">JUNPUBLISHED</option>
      <option value="2">JARCHIVED</option>
      <option value="-2">JTRASHED</option>
    </field>
    <field name="checked_out" type="hidden" filter="unset"/>
    <field name="checked_out_time" default="0000-00-00 00:00:00" type="hidden" filter="unset"/>
    <field name="dataconsegna" type="calendar" class="inputbox" label="COM_CNRBUONIPASTO_FORM_LBL_CONSEGNA_DATACONSEGNA" description="COM_CNRBUONIPASTO_FORM_DESC_CONSEGNA_DATACONSEGNA" hint="COM_CNRBUONIPASTO_FORM_LBL_CONSEGNA_DATACONSEGNA"/>
    <field name="numero" type="text" label="COM_CNRBUONIPASTO_FORM_LBL_CONSEGNA_NUMERO" description="COM_CNRBUONIPASTO_FORM_DESC_CONSEGNA_NUMERO" hint="COM_CNRBUONIPASTO_FORM_LBL_CONSEGNA_NUMERO"/>
    <field name="quantita" label="COM_CNRBUONIPASTO_FORM_LBL_CONSEGNA_QUANTITA" description="COM_CNRBUONIPASTO_FORM_DESC_CONSEGNA_QUANTITA" hint="COM_CNRBUONIPASTO_FORM_LBL_CONSEGNA_QUANTITA"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
